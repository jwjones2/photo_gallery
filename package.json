        "bootstrap": "^4.0.0",
        "jquery": "^3.4.1",
        "lodash": "^4.17.4",
        "popper.js": "^1.12",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-dropzone": "^4.2.11",
        "react-images": "^0.5.17",
        "react-photo-gallery": "^6.3.4",
        "react-router": "^5.0.1",
        "react-router-dom": "^4.3.1",
        "toastr": "^2.1.4"
    }
